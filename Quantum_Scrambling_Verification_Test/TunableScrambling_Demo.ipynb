   "metadata": {},
   "source": [
    "# Experiment A: alpha = +1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "7c009c72",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
